#include <stdio.h>
int main()
{
    int arr[20],n,i,temp;
   printf("enter the size of the array : ") ;
   scanf("%d",&n);
   printf("\nEnter the array elements : ");
   for(i=1;i<=n;i++)
   {
       scanf("%d",&arr[i]);
   }
   for(i=1;i<=n+1;i++)
   {
       if(i!=arr[i])
       {
           temp=i;
           break;
       }
       else
       {
           continue;
       }
       
   }
   printf("\nMissing element is %d",temp);
}
